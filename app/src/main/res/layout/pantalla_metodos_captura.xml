<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Blanco">

    <ImageView
        android:id="@+id/iv_LogoGrandeMetodos"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_90sdp"
        android:background="@drawable/iconofedcaza"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_weight="1" />

    <ScrollView
        android:id="@+id/sv_DatosMetodos"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintVertical_weight="2"
        android:layout_marginStart="@dimen/_10sdp"
        android:layout_marginBottom="@dimen/_10sdp"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_marginEnd="@dimen/_10sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/LinLH_BotonesGSMetodos"
        app:layout_constraintTop_toBottomOf="@+id/iv_LogoGrandeMetodos">

        <LinearLayout
            android:id="@+id/linLV_ScrollMetodosCaptura"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_IntroduceNMallas"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Introduzca el numero de mallas:"
                android:textColor="@color/VerdePrimario"
                android:textSize="@dimen/_15ssp" />

            <EditText
                android:id="@+id/etn_NumeroMallas"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="number"
                android:text=""
                android:textSize="@dimen/_15ssp"
                android:textColor="@color/Negro"/>
            <TextView
                android:id="@+id/tv_IntroduceLongitudRed"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/_15ssp"
                android:textColor="@color/VerdePrimario"
                android:text="Introduzca la longitud de la red:"/>

            <EditText
                android:id="@+id/etn_LongitudRed"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="number"
                android:textColor="@color/Negro"
                android:textSize="@dimen/_15ssp"
                android:text=""/>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="Coto:"
                    android:textColor="@color/VerdePrimario"
                    android:textSize="@dimen/_15ssp" />
                <RadioGroup
                    android:id="@+id/rbg_Coto"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:orientation="horizontal">
                    <RadioButton
                        android:id="@+id/rb_CotoSI"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textSize="@dimen/_15ssp"
                        android:textColor="@color/VerdePrimario"
                        android:text="Sí"/>
                    <RadioButton
                        android:id="@+id/rb_CotoNO"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/_15ssp"
                        android:layout_weight="1"
                        android:textColor="@color/VerdePrimario"
                        android:text="No"/>
                </RadioGroup>
            </LinearLayout>

            <Space
                android:layout_width="match_parent"
                android:layout_height="@dimen/_2sdp"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1sdp"
                android:background="@color/VerdePrimario"/>
            <Space
                android:layout_width="match_parent"
                android:layout_height="@dimen/_7sdp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_15ssp"
                    android:textColor="@color/VerdePrimario"
                    android:text="Control de agentes"/>
                <RadioGroup
                    android:id="@+id/rbg_ControlAgentes"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <RadioButton
                        android:id="@+id/rb_SiControlAgentes"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/_15ssp"
                        android:textColor="@color/VerdePrimario"
                        android:text="Si (seleccione los correspondientes)"/>
                    <CheckBox
                        android:id="@+id/cb_ControlAgentesSeprona"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_20sdp"
                        android:clickable="false"
                        android:textSize="@dimen/_15ssp"
                        android:textColor="@color/VerdePrimario"
                        android:text="Seprona"/>
                    <CheckBox
                        android:id="@+id/cb_ControlAgentesForestales"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:clickable="false"
                        android:layout_marginStart="@dimen/_20sdp"
                        android:textSize="@dimen/_15ssp"
                        android:textColor="@color/VerdePrimario"
                        android:text="Agentes Forestales"/>
                    <CheckBox
                        android:id="@+id/cb_ControlAgentesPoliciaMunicipal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:clickable="false"
                        android:layout_marginStart="@dimen/_20sdp"
                        android:textSize="@dimen/_15ssp"
                        android:textColor="@color/VerdePrimario"
                        android:text="Policia Municipal"/>
                    <CheckBox
                        android:id="@+id/cb_ControlAgentesPoliciaNacional"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:clickable="false"
                        android:layout_marginStart="@dimen/_20sdp"
                        android:textSize="@dimen/_15ssp"
                        android:textColor="@color/VerdePrimario"
                        android:text="Policia Nacional"/>
                    <CheckBox
                        android:id="@+id/cb_ControlAgentesOtros"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:clickable="false"
                        android:layout_marginStart="@dimen/_20sdp"
                        android:textSize="@dimen/_15ssp"
                        android:textColor="@color/VerdePrimario"
                        android:text="Otros"/>
                    <RadioButton
                        android:id="@+id/rb_NoControlAgentes"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/_15ssp"
                        android:textColor="@color/VerdePrimario"
                        android:text="No"/>
                </RadioGroup>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingBottom="@dimen/_10sdp"
                android:paddingTop="@dimen/_10sdp"
                android:text="Capturas y métodos de captura:"
                android:textColor="@color/VerdePrimario"
                android:textSize="@dimen/_15ssp"/>

            <HorizontalScrollView

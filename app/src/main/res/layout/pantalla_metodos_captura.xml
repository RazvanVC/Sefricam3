                            android:layout_width="@dimen/_70sdp"
                            android:layout_height="wrap_content"
                            android:text="Cim."
                            android:textAlignment="center"
                            android:textColor="@color/VerdePrimario"
                            android:textSize="@dimen/_15ssp"/>
                        <TextView
                            android:id="@+id/tv_NCapturas"
                            android:layout_width="@dimen/_70sdp"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/_15ssp"
                            android:textColor="@color/VerdePrimario"
                            android:text="Cap. M."
                            android:textAlignment="center" />
                        <TextView
